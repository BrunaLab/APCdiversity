@ARTICLE{Joung2019,
author={Joung, D. and Guo, L. and Shiller, A.M.},
title={Role of the Atchafalaya River Basin in regulating export fluxes of dissolved organic carbon, nutrients, and trace elements to the Louisiana Shelf},
