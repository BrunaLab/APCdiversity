@ARTICLE{Campos201662,
author={Campos, J.K.L. and Araújo, C.S.F. and Araújo, T.F.S. and Santos, A.F.S. and Teixeira, J.A. and Lima, V.L.M. and Coelho, L.C.B.B.},
