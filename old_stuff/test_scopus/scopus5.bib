@ARTICLE{Sanawar2018,
author={Sanawar, H. and Pinel, I. and Farhat, N.M. and Bucs, S.S. and Zlopasa, J. and Kruithof, J.C. and Witkamp, G.J. and van Loosdrecht, M.C.M. and Vrouwenvelder, J.S.},
