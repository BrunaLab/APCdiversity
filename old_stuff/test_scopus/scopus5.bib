@ARTICLE{Huber201815,
author={Huber, D. and Voith von Voithenberg, L. and Kaigala, G.V.},
title={Fluorescence in situ hybridization (FISH): History, limitations and what to expect from micro-scale FISH?},
journal={Micro and Nano Engineering},
